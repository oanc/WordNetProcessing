    <s path="file:/c:/OANC/data/written_1/letters/icic/518CWL233.anc" start="2723" end="2727" sStart="null" offset="0" sid="null" wn="2" wnkey="time%1:28:05::" text="We appreciate your taking &lt;b&gt;time&lt;/b&gt; to read about Futures for Children, I hope you are inspired to become a Sponsor and/or a Member." />
    <s path="file:/c:/OANC/data/written_1/letters/icic/518CWL233.anc" start="2972" end="2976" sStart="null" offset="0" sid="null" wn="7" wnkey="time%1:28:03::" text="Please call us any &lt;b&gt;time&lt;/b&gt; at 1-800-545-6843." />
    <s path="file:/D:/Corpora/oanc/data/written_1/letters/icic/518CWL233.anc" start="878" end="884" sStart="null" offset="0" sid="null" wn="2" wnkey="people%1:14:01::" text="The &lt;b&gt;people&lt;/b&gt; of Batavia on the Hopi Reservation in northern Arizona were motivated by their desire to rekindle traditional cultural values among younger generations when, with the help of Futures for Children, they decided to restore traditional terrace gardens at the edge of their village -gardens that had been abandoned for decades." />
    <s path="file:/D:/Corpora/oanc/data/written_1/letters/icic/518CWL233.anc" start="3065" end="3072" sStart="null" offset="0" sid="null" wn="2" wnkey="officer%1:18:02::" text="Jim West, President and Chief Executive &lt;b&gt;Officer&lt;/b&gt;" />
    <s path="file:/D:/Corpora/oanc/data/written_1/letters/icic/518CWL233.anc" start="3065" end="3072" sStart="null" offset="0" sid="null" wn="2" wnkey="officer%1:18:02::" text="Jim West, President and Chief Executive &lt;b&gt;Officer&lt;/b&gt;" />
    <s path="file:/c:/OANC/data/written_1/letters/icic/518CWL233.anc" start="1580" end="1584" sStart="null" offset="0" sid="null" wn="1" wnkey="land%1:21:01::" text="Now, as children tend their gardens, they have a new appreciation of their relationship to the &lt;b&gt;land&lt;/b&gt;, their cultural heritage, and their community." />
    <s path="file:/D:/Corpora/oanc/data/written_1/letters/icic/518CWL233.anc" start="2797" end="2803" sStart="null" offset="0" sid="null" wn="2" wnkey="become%2:42:01::" text="We appreciate your taking time to read about Futures for Children, I hope you are inspired to &lt;b&gt;become&lt;/b&gt; a Sponsor and/or a Member." />
