    <s path="[MASC]/data/written/jokes12.anc" start="3974" end="3978" sStart="3971" offset="3" sid="null" wn="1" wnkey="use%2:34:01::" text="He used olive oil" />
    <s path="[MASC]/data/written/jokes12.anc" start="1064" end="1068" sStart="null" offset="22" sid="null" wn="1" wnkey="use%2:34:01::" text="On a Japanese product used to relieve painful haemorrhoids LIE DOWN ON BED AND INSERT POSCOOL SLOWLY UP TO THE PROJECTED PORTION LIKE A SWORD-GUARD INTO ANAL DUCT." />
    <s path="[MASC]/data/written/jokes12.anc" start="541" end="545" sStart="null" offset="27" sid="null" wn="1" wnkey="use%2:34:01::" text="On a helmet mounted mirror used by US cyclists - REMEMBER, OBJECTS IN THE MIRROR ARE ACTUALLY BEHIND YOU." />
    <s path="[MASC]/data/written/jokes12.anc" start="4473" end="4479" sStart="null" offset="8" sid="null" wn="1" wnkey="try%2:41:00::" text="He kept trying to get the message across to a bunch of men who JUST DIDN'T GET IT." />
    <s path="[MASC]/data/written/jokes12.anc" start="3375" end="3379" sStart="null" offset="10" sid="null" wn="1" wnkey="name%1:10:00::" text="His first name was Jesus 2." />
