    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="4317" end="4322" sStart="null" offset="0" sid="null" wn="4" wnkey="rate%1:24:00::" annotator="cgozo" text="" />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="4317" end="4322" sStart="null" offset="0" sid="null" wn="4" wnkey="rate%1:24:00::" annotator="brubin" text="" />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="11172" end="11176" sStart="null" offset="169" sid="r8.life.n.0985" wn="1" wnkey="life%1:26:01::" annotator="veweser" text="Trying to be charitable, the best I can muster about Patrick Quigley's July 23 article, &quot;Server Time Out,&quot; is that he is simply ignorant of the facts of rural life." />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="7021" end="7025" sStart="null" offset="26" sid="r9.kill.v.0680" wn="9" wnkey="kill%2:35:02::" annotator="brubin" text="Heroin is a drug that can kill you on the very first try, and most heroin addicts didn't start out shooting up with needles--they were smoking pot as teen-agers." />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="2294" end="2298" sStart="null" offset="85" sid="r9.full.j.0607" wn="2" wnkey="full%5:00:01:whole:00" annotator="cgozo" text="(Smoking just five or more joints a day produces the same lung diseases as smoking a full pack of 20 cigarettes a day.)" />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="1250" end="1254" sStart="null" offset="72" sid="r7.1.face.n.0413" wn="3" wnkey="face%1:07:00::" annotator="veweser" text="If children see what heroin use is really like--not the somewhat benign face Stevenson puts on it--they won't use this deadly drug." />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="3615" end="3619" sStart="null" offset="28" sid="r7.1.easy.j.0147" wn="1" wnkey="easy%3:00:01::" annotator="brubin" text="Against this backdrop it is easy for young people to not buy the facts about drugs." />
    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="10281" end="10284" sStart="null" offset="193" sid="r8.ask.v.0963" wn="2" wnkey="ask%2:32:02::" annotator="brubin" text="David Plotz is absolutely right in his July 23 piece, &quot;The Secret Service's Real Secret,&quot; about the extent to which presidential protection has become absurd, but even Plotz fails to ask outright the most important question: What makes the president's life so valuable?" />
