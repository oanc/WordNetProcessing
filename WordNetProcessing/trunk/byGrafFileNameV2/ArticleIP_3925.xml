    <s path="[OANC]/data/written_1/journal/slate/47/ArticleIP_3925.anc" start="5984" end="5988" sStart="null" offset="5" sid="r10.wait.v.0487" wn="2147483646" wnkey="null" annotator="cgozo" text="Hey, Wait a Minute, Potheads!" />
