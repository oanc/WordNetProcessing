    <s path="[OANC]/data/written_1/journal/slate/54/ArticleIP_65323.anc" start="1593" end="1599" sStart="null" offset="88" sid="r7.sound.n.0455" wn="2" wnkey="sound%1:09:00::" annotator="brubin" text="To lure Christmas shoppers, the malls promise tree lightings and other &quot;sights and sounds of the holidays.&quot;" />
    <s path="[OANC]/data/written_1/journal/slate/54/ArticleIP_65323.anc" start="1582" end="1588" sStart="null" offset="77" sid="r7.sight.n.0239" wn="2" wnkey="sight%1:10:00::" annotator="brubin" text="To lure Christmas shoppers, the malls promise tree lightings and other &quot;sights and sounds of the holidays.&quot;" />
