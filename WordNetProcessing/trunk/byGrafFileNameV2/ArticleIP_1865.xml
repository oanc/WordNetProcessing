    <s path="[OANC]/data/written_1/journal/slate/30/ArticleIP_1865.anc" start="1728" end="1732" sStart="null" offset="18" sid="r7.1.face.n.0886" wn="1" wnkey="face%1:08:00::" annotator="jeneale" text="So did his doughy face, blond feathered hair, and beaky nose." />
