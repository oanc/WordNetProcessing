<list>
  <sentences>
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="2017" end="2020" sStart="1975" offset="0" sid="null" wn="5" wnkey="win%2:40:06::" annotator="veweser" text="Abbott, naturally, became a cult hero and &lt;b&gt;won&lt;/b&gt; a guest spot on Friends . (Matua walked out of the ring.)" />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="8634" end="8640" sStart="null" offset="22" sid="r7.strike.v.0380" wn="1" wnkey="strike%2:35:00::" annotator="cgozo" text="The cable TV industry struck the fatal blow." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="10556" end="10563" sStart="null" offset="31" sid="r7.strike.n.0366" wn="2" wnkey="strike%1:04:02::" annotator="jeneale" text="It banned head butts and groin strikes." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="11710" end="11715" sStart="null" offset="43" sid="r8.state.n.0477" wn="1" wnkey="state%1:15:01::" annotator="veweser" text="Away from the scrutiny of the major media, state legislators, and McCain, kids are still learning mixed martial-arts techniques, and small-time promoters are quietly staging events." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="12284" end="12289" sStart="null" offset="71" sid="r7.sight.n.0237" wn="5" wnkey="sight%1:09:02::" annotator="veweser" text="The money is low; the crowds are small; and there's not a TV camera in sight." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7937" end="7948" sStart="null" offset="33" sid="null" wn="1" wnkey="recommend%2:32:01::" annotator="vebatchelder" text="The American Medical Association recommended a ban." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7937" end="7948" sStart="null" offset="33" sid="r11.recommend.v.0286" wn="1" wnkey="recommend%2:32:01::" annotator="vebatchelder" text="The American Medical Association recommended a ban." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7937" end="7948" sStart="null" offset="33" sid="null" wn="1" wnkey="recommend%2:32:01::" annotator="anfahmy" text="The American Medical Association recommended a ban." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7937" end="7948" sStart="null" offset="33" sid="null" wn="1" wnkey="recommend%2:32:01::" annotator="adelpriore" text="The American Medical Association recommended a ban." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7132" end="7138" sStart="null" offset="25" sid="r9.kill.v.0325" wn="1" wnkey="kill%2:35:00::" annotator="veweser" text="But no one has ever been killed at the UFC--though boxers are killed every year." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7519" end="7525" sStart="null" offset="58" sid="r9.kill.v.0582" wn="1" wnkey="kill%2:35:00::" annotator="anfahmy" text="McCain sat ringside at a boxing match where a fighter was killed." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="7074" end="7080" sStart="null" offset="97" sid="r9.kill.v.0254" wn="1" wnkey="kill%2:35:00::" annotator="anfahmy" text="When I tell people I'm an ultimate fighting fan, they invariably respond: &quot;Don't people get killed all the time doing that?&quot;" />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="6525" end="6529" sStart="null" offset="56" sid="r9.kill.v.0210" wn="1" wnkey="kill%2:35:00::" annotator="anfahmy" text="As a result, they avoid the concussive head wounds that kill boxers--and the long-term neurological damage that cripples them." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="1239" end="1246" sStart="null" offset="92" sid="r9.hit.v.0614" wn="3" wnkey="hit%2:35:00::" annotator="anfahmy" text="UFC allowed, even promoted, all notions of bad sportsmanship: kicking a man when he's down, hitting him in the groin, choking." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="2787" end="2791" sStart="null" offset="9" sid="r7.1.face.n.0139" wn="2" wnkey="face%1:07:03::" annotator="brubin" text="Gracie's face grew drawn, and he squirmed wildly to avoid Severn's bombardment." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="3183" end="3195" sStart="null" offset="93" sid="r11.combination.n.0698" wn="1" wnkey="combination%1:14:00::" annotator="anfahmy" text="Nursed on boxing and Hollywood, Americans imagine fights as choreography, a dance of elegant combinations, roundhouse kicks, clean knockouts." />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="12376" end="12382" sStart="null" offset="0" sid="null" wn="2" wnkey="become%2:42:01::" annotator="veweser" text="It has &lt;b&gt;become&lt;/b&gt; Fight Club." />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="12376" end="12382" sStart="null" offset="0" sid="null" wn="2" wnkey="become%2:42:01::" annotator="tofox" text="It has &lt;b&gt;become&lt;/b&gt; Fight Club." />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="8679" end="8685" sStart="null" offset="0" sid="null" wn="2" wnkey="become%2:42:01::" annotator="carichter" text="In early 1997, McCain &lt;b&gt;became&lt;/b&gt; chairman of the commerce committee, which oversees the cable industry." />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="11118" end="11124" sStart="null" offset="0" sid="null" wn="2" wnkey="become%2:42:01::" annotator="carichter" text="Others, notably Ken Shamrock (Frank's brother), have &lt;b&gt;become&lt;/b&gt; pro wrestlers." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="5254" end="5260" sStart="null" offset="36" sid="r8.ask.v.0137" wn="4" wnkey="ask%2:32:04::" annotator="veweser" text="He sent letters to all 50 governors asking them to ban ultimate fighting." />
    <s path="[OANC]/data/written_1/journal/slate/52/ArticleIP_46344.anc" start="5254" end="5260" sStart="null" offset="36" sid="r8.ask.v.0137" wn="" wnkey="null" annotator="ksuderman" text="He sent letters to all 50 governors asking them to ban ultimate fighting." />
