    <s path="[OANC]/data/written_1/journal/slate/51/ArticleIP_38054.anc" start="4313" end="4318" sStart="null" offset="33" sid="r8.life.n.0435" wn="3" wnkey="life%1:26:02::" text="Closets are for clothes, and two lives mean split personalities." />
