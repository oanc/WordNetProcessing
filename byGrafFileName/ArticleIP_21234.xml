    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21234.anc" start="1534" end="1540" sStart="null" offset="4" sid="r11.refer.v.0790" wn="1" wnkey="refer%2:32:01::" text="She refers constantly to self-esteem and anti-depressant medication." />
