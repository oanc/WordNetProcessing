<?xml version="1.0" encoding="UTF-8"?>
<list>
  <sentences>
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="2880" end="2884" sStart="2864" offset="0" sid="null" wn="1" wnkey="warm%3:00:01::" text="First, she is a &lt;b&gt;warm&lt;/b&gt; body in bed." />
    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="4482" end="4486" sStart="null" offset="13" sid="r11.true.j.0602" wn="1" wnkey="true%3:00:00::" text="But it isn't true." />
    <s path="file:/c:/OANC/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="3954" end="3958" sStart="null" offset="0" sid="null" wn="1" wnkey="tell%2:32:04::" text="He can &lt;b&gt;tell&lt;/b&gt; her of the most trivial thing without fearing that she will think he is bothering her." />
    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="4294" end="4300" sStart="null" offset="17" sid="r7.serve.v.0358" wn="8" wnkey="serve_well%2:42:00::" text="Third, the woman serves the man's need to be needed." />
    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="2906" end="2911" sStart="null" offset="8" sid="r11.refer.v.0929" wn="1" wnkey="refer%2:32:01::" text="I don't refer to their sexual activity." />
    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="3001" end="3006" sStart="null" offset="2" sid="r11.refer.v.0569" wn="1" wnkey="refer%2:32:01::" text="I refer to something that is, if possible, even more primitive." />
    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="4654" end="4658" sStart="null" offset="0" sid="null" wn="1" wnkey="meet%2:38:01::" text="And that gives you the self-esteem to go out and &lt;b&gt;meet&lt;/b&gt; the world every day." />
    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2562.anc" start="4773" end="4778" sStart="null" offset="90" sid="r8.1.great.j.0412" wn="1" wnkey="great%5:00:01:large:00" text="So this &quot;ordinary&quot; woman--one like about 50 million others in America--has this great value to this man she is going to the theater with." />
  </sentences>
</list>