    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/30/ArticleIP_1843.anc" start="1940" end="1950" sStart="-1" offset="0" sid="null" wn="2" wnkey="suspicious%5:00:00:questionable:00" annotator="veweser" text="Tell me that's not &lt;b&gt;suspicious&lt;/b&gt; .&quot; Santa needs a makeover." />
