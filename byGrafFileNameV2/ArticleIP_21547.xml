<?xml version="1.0" encoding="UTF-8"?>
<list>
  <sentences>
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="1824" end="1828" sStart="1824" offset="0" sid="r10.wait.v.0541" wn="" wnkey="null" annotator="veweser" text="Wait." />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="3798" end="3802" sStart="null" offset="12" sid="r11.true.j.0447" wn="1" wnkey="true%3:00:00::" annotator="adelpriore" text="While it is true that all dogs go to heaven, there is a bowl curve when it comes to dog abilities and personalities." />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="5252" end="5258" sStart="null" offset="0" sid="r11.strong.j.0413" wn="2" wnkey="strong%5:00:00:intense:00" annotator="anfahmy" text="Strong genetic differences among dog breeds are not just the result of natural selection." />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="8812" end="8817" sStart="null" offset="69" sid="r7.sound.n.0395" wn="2147483645" wnkey="null" annotator="brubin" text="Since then, groups like the Animal Legal Defense Fund--does the name sound familiar?" />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="6995" end="7000" sStart="null" offset="58" sid="r9.sense.n.0163" wn="4" wnkey="sense%1:09:04::" annotator="cgozo" text="And, basset people, like me, want dogs that have the good sense not to do any of those things." />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="7843" end="7847" sStart="null" offset="81" sid="r9.kill.v.0406" wn="1" wnkey="kill%2:35:00::" annotator="anfahmy" text="Unlike, say, a German shepherd, pit bulls were not bred to protect humans but to kill other dogs." />
    <s path="[OANC]/data/written_1/journal/slate/50/ArticleIP_21547.anc" start="1144" end="1147" sStart="null" offset="8" sid="r9.hit.v.0520" wn="5" wnkey="hit%2:35:05::" annotator="brubin" text="Hardest hit by this development were dachshund and Chihuahua breeders, whose product is often sold to kids--and without warning labels of any kind." />
  </sentences>
</list>