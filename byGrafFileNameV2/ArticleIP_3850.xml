<?xml version="1.0" encoding="UTF-8"?>
<list>
  <sentences>
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="3526" end="3530" sStart="3478" offset="48" sid="r10.wait.v.0477" wn="1" wnkey="wait%2:42:00::" annotator="veweser" text="Have you ever had to fight your way through and wait in line at a Barnes &amp; Noble in Manhattan, or lived in Tokyo, where the closest Borders bookstore is in San Francisco?" />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="892" end="896" sStart="null" offset="9" sid="r10.wait.v.0475" wn="2" wnkey="wait%2:42:01::" annotator="cgozo" text="I didn't wait till the last minute." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="3379" end="3383" sStart="null" offset="23" sid="r10.wait.v.0476" wn="1" wnkey="wait%2:42:00::" annotator="brubin" text="Amazon.com online, and wait for the UPS driver to show up--while not addressing the question of convenience." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="7194" end="7198" sStart="null" offset="30" sid="r10.wait.v.0478" wn="1" wnkey="wait%2:42:00::" annotator="anfahmy" text="Did I mention I never have to wait in line at Amazon?" />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="1760" end="1766" sStart="null" offset="76" sid="r7.strike.v.0290" wn="2147483647" wnkey="null" annotator="jeneale" text="My girlfriend was attempting to find a book for a Christmas present and had struck out at about half a dozen bookstores around Manhattan." />
    <s path="file:/c:/OANC/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="428" end="432" sStart="null" offset="0" sid="null" wn="2147483646" wnkey="null" annotator="rstandig" text="Bean, &lt;b&gt;Land&lt;/b&gt;'s End, and J. Crew on the Web." />
    <s path="file:/c:/OANC/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="428" end="432" sStart="null" offset="0" sid="null" wn="2147483645" wnkey="null" annotator="brubin" text="Bean, &lt;b&gt;Land&lt;/b&gt;'s End, and J. Crew on the Web." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="2424" end="2431" sStart="null" offset="26" sid="r10.find.v.0761" wn="3" wnkey="find%2:40:00::" annotator="veweser" text="They didn't work magic by finding books bookstores couldn't, but they did deliver on their promises, and Amazon certainly presents some interesting browsing methods and far better search facilities than most bookstores." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="412" end="416" sStart="null" offset="29" sid="r10.find.v.0529" wn="3" wnkey="find%2:40:00::" annotator="veweser" text="--a sales pitch for where to find L.L." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="5815" end="5819" sStart="null" offset="25" sid="r7.1.face.n.0173" wn="1" wnkey="human_face%1:08:01::" annotator="veweser" text="By the expression on his face I knew for sure I had blundered the syntax this time." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="6761" end="6766" sStart="null" offset="28" sid="r10.book.n.0485" wn="1" wnkey="book%1:10:00::" annotator="veweser" text="And now, after selecting my books, it takes me less than a minute to order." />
    <s path="[OANC]/data/written_1/journal/slate/46/ArticleIP_3850.anc" start="2327" end="2332" sStart="null" offset="11" sid="r10.book.n.0357" wn="2 1" wnkey="null" annotator="veweser" text="We got the books in a timely fashion." />
  </sentences>
</list>