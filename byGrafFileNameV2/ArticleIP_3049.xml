    <s path="file:/D:/Corpora/oanc/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="3933" end="3937" sStart="null" offset="0" sid="null" wn="1" wnkey="lose%2:40:02::" annotator="veweser" text="It is a sealed well that doesn't &lt;b&gt;lose&lt;/b&gt; a drop, to use an old expression." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2368" end="2372" sStart="null" offset="8" sid="r8.live.v.0066" wn="4" wnkey="live%2:42:01::" annotator="veweser" text="One can live on trains." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2368" end="2372" sStart="null" offset="8" sid="r8.live.v.0066" wn="1" wnkey="live%2:42:08::" annotator="jeneale" text="One can live on trains." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2368" end="2372" sStart="null" offset="8" sid="r8.live.v.0066" wn="1" wnkey="live%2:42:08::" annotator="cgozo" text="One can live on trains." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="4609" end="4614" sStart="null" offset="3" sid="r8.live.v.0515" wn="4" wnkey="live%2:42:01::" annotator="brubin" text="He lives by buying up old ceremonial objects retrieved from synagogues nearly destroyed by the Nazis." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="835" end="840" sStart="null" offset="95" sid="r8.live.v.0479" wn="2147483645" wnkey="null" annotator="brubin" text="Appelfeld, a survivor from Czernowitz in Austrian Galicia whose first language was German, now lives in Jerusalem and writes in Hebrew." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2368" end="2372" sStart="null" offset="8" sid="r8.live.v.0066" wn="1" wnkey="live%2:42:08::" annotator="brubin" text="One can live on trains." />
    <s path="file:/c:/OANC/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2163" end="2167" sStart="null" offset="0" sid="null" wn="4" wnkey="know%2:31:00::" annotator="kaolla" text="Now I &lt;b&gt;know&lt;/b&gt; every hostel and every inn, every restaurant and buffet, the vehicles that bring you to the remotest corners." />
    <s path="file:/c:/OANC/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="2163" end="2167" sStart="null" offset="0" sid="null" wn="4" wnkey="know%2:31:00::" annotator="carichter" text="Now I &lt;b&gt;know&lt;/b&gt; every hostel and every inn, every restaurant and buffet, the vehicles that bring you to the remotest corners." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="5596" end="5600" sStart="null" offset="20" sid="r9.kill.v.0377" wn="1" wnkey="kill%2:35:00::" annotator="veweser" text="Siegelbaum wants to kill the man for revealing his hidden desire." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="1781" end="1785" sStart="null" offset="37" sid="r9.kill.v.0139" wn="1" wnkey="kill%2:35:00::" annotator="cgozo" text="His purpose is to find Nachtigel and kill him." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="5701" end="5708" sStart="null" offset="30" sid="r9.kill.v.0980" wn="1" wnkey="kill%2:35:00::" annotator="brubin" text="There is nothing simpler than killing a man, and yet for some reason, I cannot do it.&quot;" />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="1762" end="1766" sStart="null" offset="18" sid="r10.find.v.0543" wn="3" wnkey="find%2:40:00::" annotator="cgozo" text="His purpose is to find Nachtigel and kill him." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="6175" end="6179" sStart="null" offset="10" sid="r10.find.v.0780" wn="2" wnkey="find%2:39:02::" annotator="brubin" text="he cannot find an answer within himself." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="4081" end="4086" sStart="null" offset="85" sid="r7.1.face.n.0322" wn="1 5" wnkey="null" annotator="jeneale" text="My memory is a powerful machine that stores and constantly discharges lost years and faces." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="6051" end="6055" sStart="null" offset="7" sid="r7.1.face.n.0228" wn="2147483647" wnkey="null" annotator="cgozo" text="In the face of prosperous postwar Germany, retribution is a joke." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="6408" end="6412" sStart="null" offset="60" sid="r7.1.face.n.0491" wn="2147483646" wnkey="null" annotator="brubin" text="Its real name is despair, that central human emotion in the face of overwhelming odds of which tragedy is made." />
    <s path="[OANC]/data/written_1/journal/slate/41/ArticleIP_3049.anc" start="3189" end="3194" sStart="null" offset="88" sid="r10.board.n.0081" wn="2147483645" wnkey="null" annotator="veweser" text="He is so frozen inside that he remains completely detached from the occasional woman on board who attracts him:" />
