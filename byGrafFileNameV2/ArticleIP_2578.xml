    <s path="[OANC]/data/written_1/journal/slate/37/ArticleIP_2578.anc" start="531" end="537" sStart="null" offset="27" sid="r7.strike.v.0237" wn="2" wnkey="strike%2:37:00::" annotator="veweser" text="More recently, I have been struck by the smoking." />
